<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Dog_Nose_print_Search_list_Success_Shelter">

    <TextView
        android:id="@+id/tv_search_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="비문조회 결과"
        android:textColor="#5C5B5B"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_success_shelter"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:textColor="#5C5B5B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_search_success"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/tv_find_shelter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="'살구'를 찾았어요!"
        android:textColor="#5C5B5B"
        android:textSize="24sp"
        android:fontFamily="@font/nanumbarunpenb"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_success_shelter" />

    <TextView
        android:id="@+id/tv_call_owner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="견주에게 연락해보세요!"
        android:textColor="#5C5B5B"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_find_shelter" />

    <TextView
        android:id="@+id/tv_owner_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="95dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="견주 이름"
        android:textColor="#5C5B5B"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_call_owner" />

    <TextView
        android:id="@+id/tv_owner_tel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="95dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="견주 전화번호"
        android:textColor="#5C5B5B"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_owner_name" />

    <TextView
        android:id="@+id/tv_owner_name1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="208dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="이창현"
        android:textColor="#5C5B5B"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_call_owner" />

    <TextView
        android:id="@+id/tv_owner_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="208dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="010-1234-5678"
        android:textColor="#5C5B5B"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_owner_name1" />


</androidx.constraintlayout.widget.ConstraintLayout>