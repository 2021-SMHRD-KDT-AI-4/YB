<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fontFamily="@font/nanumbarunpenb"
    tools:context=".F_Dog_Write_Result">

    <TextView
        android:id="@+id/tv_f_s_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="268dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="전화하기"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgbtn_f_s_tel" />

    <TextView
        android:id="@+id/tv_f_s_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="정보보기"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgbtn_f_s_info" />

    <TextView
        android:id="@+id/tv_f_dog_shelter_alarm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="주변에 가까운 보호소가 있어요"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_d_result_tel" />

    <TextView
        android:id="@+id/tv_f_dog_shelter_alarm2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="연락해볼까요?"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_dog_shelter_alarm" />

    <TextView
        android:id="@+id/tv_f_d_result_kind"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="푸들"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_f_dog_picture" />

    <TextView
        android:id="@+id/tv_f_d_result_day"
        android:layout_width="120dp"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="2021.08.10"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_d_result_kind" />

    <TextView
        android:id="@+id/tv_f_d_result_place"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="스마트인재개발원"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_d_result_day" />

    <TextView
        android:id="@+id/tv_f_d_result_time"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="오전 9시"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_d_result_place" />

    <TextView
        android:id="@+id/tv_f_d_result_tel"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="01012341234"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_d_result_time" />

    <TextView
        android:id="@+id/tv_f_dog_write_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="게시글 등록이 완료되었습니다!"
        android:textColor="#706E6E"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_f_dog_picture"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_dog_write_result"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/f_d_result_kind"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="품종"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_f_dog_picture" />

    <TextView
        android:id="@+id/f_d_result_day"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="목격날짜"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_d_result_kind" />

    <TextView
        android:id="@+id/f_d_result_place"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="목격위치"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_d_result_day" />

    <TextView
        android:id="@+id/f_d_result_time"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="목격시간"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_d_result_place" />

    <TextView
        android:id="@+id/f_d_result_tel"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="연락처"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f_d_result_time" />

    <ImageButton
        android:id="@+id/imgbtn_f_s_info"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_dog_shelter_alarm2"
        app:srcCompat="@drawable/s_info" />

    <ImageButton
        android:id="@+id/imgbtn_f_s_tel"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="260dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_dog_shelter_alarm2"
        app:srcCompat="@drawable/s_tel" />

    <ImageButton
        android:id="@+id/imgbtn_f_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_f_dog_shelter_alarm2"
        app:srcCompat="@drawable/home" />
    <!--        app:srcCompat="@drawable/home" />-->

</androidx.constraintlayout.widget.ConstraintLayout>