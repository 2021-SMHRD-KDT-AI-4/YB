<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/imgbtn_l_home"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    tools:context=".L_Dog_Write_Result">

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_dog_shelter_alarm2"
        app:srcCompat="@drawable/home" />

    <TextView
        android:id="@+id/tv_l_dog_write_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="게시글 등록이 완료되었습니다!"
        android:textColor="#706E6E"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_l_dog_picture2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_dog_write_result"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/l_d_result_kind"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="품종"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_l_dog_picture2" />

    <TextView
        android:id="@+id/tv_l_d_result_day"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="2021.07.27"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_d_result_kind" />

    <TextView
        android:id="@+id/tv_l_d_result_time"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="오후 11시"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_d_result_place" />

    <TextView
        android:id="@+id/tv_l_d_result_kind"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="푸들"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_l_dog_picture2" />

    <TextView
        android:id="@+id/tv_l_d_result_kind2"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="푸들"
        android:textColor="#7C7C7C"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_l_dog_picture2" />

    <TextView
        android:id="@+id/tv_l_d_result_tel"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="01012341234"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_d_result_time" />

    <TextView
        android:id="@+id/tv_l_d_result_place"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="204dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="조선대학교 후문"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_d_result_day" />

    <TextView
        android:id="@+id/l_d_result_day"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="실종날짜"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/l_d_result_kind" />

    <TextView
        android:id="@+id/l_d_result_place"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="실종위치"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/l_d_result_day" />

    <TextView
        android:id="@+id/l_d_result_time"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="실종시간"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/l_d_result_place" />

    <TextView
        android:id="@+id/l_d_result_tel"
        android:layout_width="75dp"
        android:layout_height="27dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="6dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="연락처"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/l_d_result_time" />

    <TextView
        android:id="@+id/tv_l_dog_shelter_alarm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="주변에 가까운 보호소가 있어요"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/l_d_result_tel" />

    <TextView
        android:id="@+id/tv_l_s_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="정보보기"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgbtn_l_s_info" />

    <TextView
        android:id="@+id/tv_l_s_tel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="273dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="전화하기"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgbtn_l_s_tel" />

    <ImageButton
        android:id="@+id/imgbtn_l_s_info"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_dog_shelter_alarm2"
        app:srcCompat="@drawable/s_info" />

    <TextView
        android:id="@+id/tv_l_dog_shelter_alarm2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/nanumbarunpenb"
        android:text="연락해볼까요?"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_dog_shelter_alarm" />

    <ImageButton
        android:id="@+id/imgbtn_l_s_tel"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="265dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_l_dog_shelter_alarm2"
        app:srcCompat="@drawable/s_tel" />

</androidx.constraintlayout.widget.ConstraintLayout>